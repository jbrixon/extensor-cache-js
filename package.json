{
  "name": "extensor-cache",
  "version": "1.1.3",
  "description": "Simple caching library.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "COPYING"
  ],
  "scripts": {
  "build": "tsc",
  "build:watch": "tsc --watch",
  "type-check": "tsc --noEmit",
  "test": "npm run test:unit",
  "test:unit": "jest --silent --detectOpenHandles --testPathPattern=tests/unit",
  "test:acceptance": "jest --silent --detectOpenHandles --testPathPattern=tests/acceptance",
  "lint": "eslint src/ tests/",
  "lint:fix": "eslint src/ tests/ --fix",
  "format": "prettier --write \"src/**/*.{js,ts}\" \"tests/**/*.{js,ts}\"",
  "format:check": "prettier --check \"src/**/*.{js,ts}\" \"tests/**/*.{js,ts}\""
  },
  "keywords": [
    "caching",
    "cache",
    "in-memory",
    "ttl",
    "memory-cache"
  ],
  "contributors": [
    {
      "name": "Joe Rixon",
      "email": "joe@fabrik.software"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jbrixon/extensor-cache-js.git"
  },
  "author": "Joe Rixon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jbrixon/extensor-cache-js/issues"
  },
  "homepage": "https://github.com/jbrixon/extensor-cache-js#readme",
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "babel-jest": "^29.5.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^29.5.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
